[user]
    email = panswiata@gmail.com
    name = Jakub Soko≈Çowski
[core]
    editor = vim
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[alias]
    hist = log --graph
    co = checkout
    b = branch
    ps = push
    pl = pull
    sm = submodule
    c = commit --verbose
    a = add 
    l  = log --decorate
    sl = log --graph --pretty=format:'%h : %s'
    df = diff
    dt = difftool
[branch "master"]
    remote = origin
    merge = refs/heads/master
[credential]
    helper = cache --timeout=1800
[color]
    ui = true
[push]
    default = current
[pull]
    default = current
[difftool]
    prompt = false
