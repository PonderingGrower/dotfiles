[user]
    email = jacob@codility.com
    name = Jakub Soko≈Çowski
[core]
    editor = vim
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[alias]
    hist = log --graph
    co = checkout
    b = branch
    f = fetch
    ps = push
    pl = pull
    sm = submodule
    s = stash
    c = commit --verbose
    a = add 
    an = annex
    l  = log --decorate
    sl = log --graph --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    df = diff
    dt = difftool
[branch "master"]
    remote = origin
    merge = refs/heads/master
[credential]
    helper = cache --timeout=1800
[color]
    ui = true
[push]
    default = current
[pull]
    default = current
[difftool]
    prompt = false
